en((res) => {\r\n        that.hotPlaylistTag = res.data.tags\r\n      })\r\n    },\r\n    //获取全部歌单分类\r\n    getAllPlayList() {\r\n      let that = this\r\n      getAllPlayList().then((res) => {\r\n        // console.log(res)\r\n        that.categories = res.data.categories\r\n        for (let key in that.categoryTag) {\r\n          res.data.sub.forEach((item) => {\r\n            if (item.category == key) {\r\n              that.categoryTag[key].push(item)\r\n              console.log(that.categoryTag)\r\n            }\r\n          })\r\n        }\r\n      })\r\n    },\r\n    //获取歌单列表----\r\n    getPlaylist(cat) {\r\n      let that = this\r\n      let params = {\r\n        cat: cat,\r\n        limit: 20,\r\n        offset: 0,\r\n      }\r\n      getPlaylist(params).then((res) => {\r\n        console.log(res)\r\n        that.playListTable = res.data.playlists\r\n        // that.count = res.data.total\r\n        // transPlayCount(that.playListTable)\r\n      })\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.descript {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  color: aliceblue;\r\n  font-weight: bolder;\r\n  padding: 10px 20px;\r\n  border-radius: 20px 20px 0 0;\r\n  background: rgba(95, 158, 160, 0.05);\r\n  box-sizing: border-box;\r\n}\r\n.list-wrap {\r\n  margin: 10px auto;\r\n  padding: 0 20px 10px;\r\n  width: 1120px;\r\n  height: 730px;\r\n  border-radius: 20px;\r\n  background: rgba(95, 158, 160, 0.11);\r\n  box-sizing: border-box;\r\n\r\n  .hotCategary {\r\n    padding: 0 5px;\r\n    font-size: 15px;\r\n    color: rgb(201, 185, 165);\r\n    // border: 1px dotted rgb(192, 190, 190);\r\n    // border-radius: 10px;\r\n    transition: 0.3s;\r\n\r\n    &:hover {\r\n      cursor: pointer;\r\n      color: rgb(202, 162, 108);\r\n    }\r\n  }\r\n  .categary-select {\r\n    padding: 0 10px;\r\n    // border: 1px solid #eee;\r\n    border-radius: 10px;\r\n\r\n    color: rgb(201, 185, 165);\r\n    transition: 0.3s;\r\n    &:hover {\